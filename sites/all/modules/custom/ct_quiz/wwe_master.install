  $endpoint->api_version = 3;
  $endpoint->name = 'drupalapi';
  $endpoint->server = 'rest_server';
  $endpoint->path = 'api/v1';
  $endpoint->authentication = [
    'oauth2_server' => [
      'server' => 'mdm',
    ],
  ];
  $endpoint->server_settings = [
    'formatters' => [
      'json' => TRUE,
      'xml' => TRUE,
